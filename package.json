{
  "name": "business-logic-server-template",
  "description": "Business Logic Server Template is a generic open-source server that encodes the real-world business rules that determine how data can be created, stored, and changed. It saves you from rewriting boilerplate code. It works best with other Open Template Hub products.",
  "version": "4.0.1",
  "license": "MIT",
  "main": "business-logic-server.main.ts",
  "scripts": {
    "start": "ts-node business-logic-server.main.ts",
    "startLocal": "nodemon --exec DEBUG=* ts-node business-logic-server.main.ts",
    "postmanDevelop": "newman run ./assets/tests/regression-tests/postman/business-logic-server-regression.postman_collection.json -e ./assets/tests/regression-tests/postman/business-logic-server-regression.postman_environment.json -r htmlextra --reporter-htmlextra-export ./assets/test-results/postman.html --reporter-htmlextra-darkTheme  > ./assets/test-results/postman-dark.html",
    "outdated": "node dependency-checker.ts > docs/OUTDATED.md"
  },
  "dependencies": {
    "@open-template-hub/common": "^4.0.1",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.5.34",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-promise-router": "^4.1.1",
    "mongoose": "^6.2.10",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "git repository": "https://github.com/open-template-hub/business-logic-server-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-template-hub/business-logic-server-template.git"
  },
  "keywords": [
    "info",
    "information",
    "business-logic",
    "mongo",
    "mongodb",
    "mongoose",
    "backend",
    "rest",
    "node",
    "nodejs",
    "typescript",
    "template",
    "server template",
    "open template hub"
  ]
}
